t { apiService } from \"../common/api.service.js\"\n\nexport default {\n    name: 'job',\n    props: {\n        id: {\n            type: Number,\n            required: true\n        }\n    },\n    data() {\n        return {\n            job: {}\n        }\n    },\n    methods: {\n        setPageTitle(title) {\n            document.title = title;\n        },\n        getJobData() {\n            let endpoint = `/api/jobs/${this.id}/`;\n            apiService(endpoint).then(data => {\n                this.job = data;\n                this.setPageTitle(data.company_name);\n            });\n        },\n        deleteJobData() {\n            let endpoint = `/api/jobs/${this.id}/`;\n            apiService(endpoint, \"DELETE\").then(() => {\n                this.$router.push({\n                    name: 'home'\n                });\n            });\n        }\n    },\n    created() {\n        this.getJobData()\n    }\n}\n</script>\n\n<style>\n\n</style>"]}]}